	{
		real_t U_local = U(-1,0);
		real_t invU = 1.0 / (1 + U_local) ;
		<?R if (Options$Outflow){
			C(g, (g_old + U_loc*g_n)*U_inv)
			C(h, (h_old + U_loc*h_n)*U_inv)

			C(g_old,g)
			C(h_old,h)}
		?>

	}

